<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".deleteWorkingHour">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:background="#662196F3"
        android:orientation="vertical">

        <Button
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#C2185B"
            android:onClick="deleteWH"
            android:text="Finish"
            android:textColor="#FFFFFF" />

        <CalendarView
            android:id="@+id/calendarView3"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:maxDate="12/31/2200"
            android:minDate="01/01/2000" />

        <CalendarView
            android:id="@+id/calendarView4"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:maxDate="12/31/2200"
            android:minDate="01/01/2000" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>